{
  "name": "frontend-ng",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark && npm run open-api-generate && svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "open-api-generate": "openapi --input ./src/backend_api.json --output ./src/gen --postfix \"\""
  },
  "devDependencies": {
    "@playwright/test": "^1.19.1",
    "@smui-extra/autocomplete": "^6.0.0-beta.15",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/checkbox": "^6.0.0-beta.15",
    "@smui/data-table": "^6.0.0-beta.15",
    "@smui/dialog": "^6.0.0-beta.15",
    "@smui/drawer": "^6.0.0-beta.15",
    "@smui/fab": "^6.0.0-beta.15",
    "@smui/form-field": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.15",
    "@smui/layout-grid": "^6.0.0-beta.15",
    "@smui/paper": "^6.0.0-beta.15",
    "@smui/radio": "^6.0.0-beta.15",
    "@smui/select": "^6.0.0-beta.15",
    "@smui/snackbar": "^6.0.0-beta.15",
    "@smui/tab": "^6.0.0-beta.15",
    "@smui/tab-bar": "^6.0.0-beta.15",
    "@smui/textfield": "^6.0.0-beta.15",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "openapi-typescript-codegen": "^0.20.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module"
}
